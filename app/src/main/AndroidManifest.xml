<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.ders_takibi">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher5"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon">
        <activity android:name="com.example.ders_takibi.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.ders_takibi.Ogrenci_Duzenle"
            android:parentActivityName="com.example.ders_takibi.Ogrenci_Listesi"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name="com.example.ders_takibi.Ogrenci_Listesi"
            android:parentActivityName="com.example.ders_takibi.Anasayfa"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name="com.example.ders_takibi.Ogrenci_Durumu"
            android:parentActivityName="com.example.ders_takibi.Durum_Goruntule"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name="com.example.ders_takibi.Durum_Goruntule"
            android:parentActivityName="com.example.ders_takibi.Anasayfa"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name="com.example.ders_takibi.Ogrenci_Ekle"
            android:parentActivityName="com.example.ders_takibi.Anasayfa"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name="com.example.ders_takibi.Odev_Ekle"
            android:parentActivityName="com.example.ders_takibi.Anasayfa"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name="com.example.ders_takibi.Anasayfa"
            android:windowSoftInputMode="adjustPan" />
    </application>

</manifest>